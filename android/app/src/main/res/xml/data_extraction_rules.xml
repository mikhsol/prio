<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include preferences and database -->
        <include domain="sharedpref" path="." />
        <include domain="database" path="." />
        <exclude domain="database" path="jeeves_database-journal" />
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="." />
        <include domain="database" path="." />
    </device-transfer>
</data-extraction-rules>
